    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "antd": "^5.4.6",
    "axios": "^1.1.3",
    "env-cmd": "^10.1.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-local": "set PORT=5400 && react-scripts start",
    "start-mock": "set PORT=5200 && react-scripts start",
    "start:local": "env-cmd -f environment/.env.local npm run-script start-local",
    "start:mock": "env-cmd -f environment/.env.mock npm run-script start-mock",
    "deploy:dev": "env-cmd -f environment/.env.dev npm run-script deploy",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "mock": "nodemon --ext ts,json --watch mock-server.js --watch mock-server/db.json ts-node mock-server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "json-server": "^0.17.3",
    "node-sass": "^7.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "sass": "^1.62.1"
  }
